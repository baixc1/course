page{  
  background: #f5f5f5;  
}  
/*总体主盒子*/  
.container {  
  color: #939393;  
  position:relative;
}  
  
/*左侧栏主盒子*/  
.nav_left{  
  width: 25%;  
  text-align: center;  
}  
/*左侧栏list的item*/  
.nav_left .nav_left_items{  
  height: 30px;  
  line-height: 30px;  
  padding: 6px 0;  
  border-bottom: 1px solid #dedede;  
  font-size: 14px;  
}  
/*左侧栏list的item被选中时*/  
.nav_left .nav_left_items.active{  
  background: #fff;  
}  
  
/*右侧栏主盒子*/  
.nav_right{  
  position: absolute;  
  top: 0;  
  right: 0;  
  flex: 1;  
  width: 75%;  
  height: 100%;  
  padding: 10px;  
  box-sizing: border-box;  
  background: #fff;  
}  
/*右侧栏list的item*/  
.nav_right .nav_right_items{  
  float: left;  
  width: 33.33%;  
  height: 80px;  
  text-align: center;  
}  
.nav_right .nav_right_items image{  
  width: 50px;  
  height: 30px;  
}  
.nav_right .nav_right_items view{  
  margin-top: 5px;  
  font-size: 10px;  
  overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
}  

/* Classification of the entrance */
.menu-wrp {  
  margin-top:20rpx;  
  overflow: hidden;
}  
  
.menu-list{  
    float:left;  
    width:20%;  
    padding-bottom:10px;  
}  
  
.menu-img{  
    width:120rpx;  
    height:84rpx;  
    display:block;  
    margin: auto;  
    margin-bottom:5px;  
}  
.menu-desc{   
    color:#333333;  
    text-align: center;  
    font-size:12px;  
}  